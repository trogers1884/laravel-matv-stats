{
  "name": "trogers1884/laravel-matv-stats",
  "description": "A Laravel package for monitoring materialized view refresh performance in PostgreSQL",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Rogers",
      "email": "trogers1884@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0",
    "friendsofphp/php-cs-fixer": "^3.65"
  },
  "autoload": {
    "psr-4": {
      "Trogers1884\\LaravelMatVStats\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trogers1884\\LaravelMatVStats\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trogers1884\\LaravelMatVStats\\MatVStatsServiceProvider"
      ],
      "aliases": {
        "MatVStats": "Trogers1884\\LaravelMatVStats\\Facades\\MatVStats"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
